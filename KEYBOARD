 // key <RTRN> { [ m, M, Delete, Delete, percent] };

 key <LFSH> { [ w, W] }; //
 key <LSGT> { [ z, Z, U2202 ] };
 key <AB01> { [ agrave, Agrave, dead_grave, dead_grave, asciicircum ] };
